{
  "name": "dj-ino",
  "version": "0.0.1",
  "description": "Discord bot that can be used to play music in a voice channel",
  "main": "index.js",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "start": "node dist/index.js",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "deploy-commands": "node bin/deploy-commands.js",
    "watch": "tsc -p tsconfig.json -w",
    "build": "tsc -p tsconfig.json",
    "build:dev": "docker build --build-arg ENV=DEV -t djinodev .",
    "test:unit": "jest --coverage --config=jest.config.js",
    "test:integration": "jest --coverage --config=jest.integration.config.js"
  },
  "author": "alpacamybags",
  "license": "ISC",
  "dependencies": {
    "@alpacamybags118/yt-dlp-exec": "^0.1.0",
    "@discordjs/voice": "^0.7.5",
    "@types/node": "^17.0.4",
    "discord.js": "^13.3.0",
    "dotenv": "^10.0.0",
    "ffmpeg-static": "^4.4.0",
    "i": "^0.3.7",
    "inversify": "^6.0.1",
    "jest": "^27.4.5",
    "libsodium-wrappers": "^0.7.9",
    "node-fetch": "^2.6.6",
    "npm": "^8.3.0",
    "pino": "^7.6.1",
    "pino-pretty": "^7.3.0",
    "reflect-metadata": "^0.1.13",
    "typescript": "^4.5.4"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/plugin-proposal-decorators": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@discordjs/rest": "^0.2.0-canary.0",
    "@types/jest": "^27.0.3",
    "@types/node-fetch": "^2.5.12",
    "babel-jest": "^27.4.5",
    "ts-jest": "^27.1.2",
    "ts-mockito": "^2.6.1",
    "ts-node": "^10.4.0"
  }
}
